{
    "name": "osik/hubtel-laravel-sms",
    "description": "A Laravel package for sending SMS via Hubtel API",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Osik\\HubtelLaravelSms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Osik\\HubtelLaravelSms\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Osik\\HubtelLaravelSms\\HubtelSmsServiceProvider"
            ],
            "aliases": {
                "HubtelSms": "Osik\\HubtelLaravelSms\\Facades\\HubtelSms"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}